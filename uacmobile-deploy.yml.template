apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: uacmobile
spec:
  replicas: 1
  template:
    metadata:
      labels:
        app: uacmobile
    spec:
      containers:
      - name: uacmobile
        image: wcr.io/zteuac/uacmobile:$TAG
        ports:
        - containerPort: 80
        resources:
          limits:
            memory: 4Gi
        imagePullPolicy: Always
      imagePullSecrets:
      - name: wrelease
